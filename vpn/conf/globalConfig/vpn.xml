<?xml version="1.0" encoding="UTF-8"?>
<globalConfig xmlns="http://syscxp.com/schema/syscxp">
    <config>
        <category>vpn</category>
        <name>transferRpcIp</name>
        <description>TRANSFER_RPC_IP</description>
        <defaultValue>192.168.211.6</defaultValue>
        <type>java.lang.String</type>
    </config>
    <config>
        <category>vpn</category>
        <name>falconApiIp</name>
        <description>FALCON_API_IP</description>
        <defaultValue>192.168.211.6</defaultValue>
        <type>java.lang.String</type>
    </config>

    <config>
        <category>vpn</category>
        <name>expiredVpn.deleteInterval</name>
        <description>expired vpn delete interval, in days</description>
        <defaultValue>10</defaultValue>
        <type>java.lang.Integer</type>
    </config>
    <config>
        <category>vpn</category>
        <name>expiredVpn.closeInterval</name>
        <description>expired product close interval, in days</description>
        <defaultValue>5</defaultValue>
        <type>java.lang.Integer</type>
    </config>
    <config>
        <category>vpn</category>
        <name>expiredVpnClean.run</name>
        <description>A boolean value, enabled expired vpn clean</description>
        <defaultValue>false</defaultValue>
        <type>java.lang.Boolean</type>
    </config>

    <config>
        <category>vpn</category>
        <name>expiredVpn.cleanInterval</name>
        <description>expired vpn clean process run interval, in hours</description>
        <defaultValue>12</defaultValue>
        <type>java.lang.Integer</type>
    </config>
</globalConfig>
