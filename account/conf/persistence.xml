<persistence xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd"
             version="2.0">
    <persistence-unit name="zstack.jpa" transaction-type="RESOURCE_LOCAL">
        <mapping-file>persistence-mapping.xml</mapping-file>

        <class>org.zstack.core.config.GlobalConfigVO</class>
        <class>org.zstack.header.managementnode.ManagementNodeVO</class>
        <class>org.zstack.core.job.JobQueueVO</class>
        <class>org.zstack.core.job.JobQueueEntryVO</class>

        <class>org.zstack.account.header.account.AccountVO</class>
        <class>org.zstack.account.header.user.UserVO</class>
        <class>org.zstack.account.header.identity.RoleVO</class>
        <class>org.zstack.account.header.user.UserRoleRefVO</class>
        <class>org.zstack.account.header.identity.SessionVO</class>
        <class>org.zstack.account.header.account.AccountExtraInfoVO</class>
        <class>org.zstack.account.header.identity.PolicyVO</class>
        <class>org.zstack.account.header.account.AccountContactsVO</class>
        <class>org.zstack.account.header.account.AccountApiSecurityVO</class>
        <class>org.zstack.account.header.account.ProxyAccountRefVO</class>

        <class>org.zstack.account.header.log.NoticeVO</class>
        <class>org.zstack.sms.header.SmsVO</class>
        <class>org.zstack.account.header.log.AlarmContactVO</class>
        <class>org.zstack.core.notification.NotificationVO</class>

        <class>org.zstack.account.header.ticket.TicketVO</class>
        <class>org.zstack.account.header.ticket.TicketRecordVO</class>
        <class>org.zstack.account.header.ticket.TicketTypeVO</class>

    </persistence-unit>
</persistence>
